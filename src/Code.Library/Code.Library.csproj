    <Compile Remove="Helpers\DebugHelper.cs" />
    <Compile Remove="Helpers\EmailHelper.cs" />
    <Compile Remove="Helpers\ExceptionHelper.cs" />
    <Compile Remove="Helpers\ImageHelper.cs" />
    <Compile Remove="Helpers\NetworkHelper.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="log.txt" />
  </ItemGroup>

</Project>
