<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Version>2.2.0</Version>
    <Authors>Abhith Rajan</Authors>
    <Description>C# utils libraries contain a collection of reusable models, helper methods and extensions.</Description>
    <PackageProjectUrl>https://code-library.abhith.net</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Abhith/Code.Library</RepositoryUrl>
    <PackageReleaseNotes># Added

- Extension method to get last N characters from a string (GetLast(N))</PackageReleaseNotes>
    <PackageTags>c#, helper</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
